<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
<bean name="emp" class="com.tns.springcore.collections.Emp">
<property name="name" value="Manjunath"></property>
<property name = "phones">
	<list>
	<value>8152063656</value>
	<value>34567856</value>
	<value>456878654</value>
	</list>
</property>
<property name="addresses">
	<set>
		<value>bangalore</value>
		<value>gulbarga</value>
		<value>mangalore</value>
	</set>
</property>
<property name="courses">
	<map>
		<entry key="java" value="2month"></entry>
		<entry key="python" value="2month"></entry>
		<entry key="c" value="2month"></entry>
	</map>	
</property>
</bean>
    

</beans>