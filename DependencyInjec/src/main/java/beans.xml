<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    
    <bean id="s1" class="com.tns.Di.Student">
    <property name = "studentName" value ="Manjunath"></property>
    <property name = "id" value ="19"></property>
    <property name="studentAddress" value="Bangaluru"></property></bean>
    
    <bean name="s2" class="com.tns.Di.Student" p:id="10" p:studentName="kumar" p:studentAddress="Bangalore" ></bean>
   <!--  <constructor-arg name="studentName" value="Kumar"></constructor-arg>
    <constructor-arg name="id" value="10"></constructor-arg>-->

</beans>